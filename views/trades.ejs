<%- include('./partials/header.ejs')%>

<div class="roasts-container">
  <% roasts.map(roast => { %>
  <div class="roast-card">
    <div class="roast-title"><%= roast %></div>
    <div class="roast-card-trades">
      <% trades.filter(trade => trade.roastType === roast).map(trade => { %>
      <div><%= trade.coffeeName %></div>
      <% }); %>
    </div>
  </div>
  <% }); %>
</div>

<%- include('./partials/footer.ejs')%>
